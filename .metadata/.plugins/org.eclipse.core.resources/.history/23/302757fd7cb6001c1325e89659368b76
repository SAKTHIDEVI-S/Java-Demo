import java.util.Scanner;
public class ExotnTryCatch {
	public static void main(String []args) {
		try {
			Scanner sc = new Scanner(System.in);
			num1 = sc.nextInt();
			num2 = sc.nextInt();
			num3 = num1+num2;
		} catch (Exception ex) {
			System.out.println("Addition can be made only with integers");
		}
	}

}
